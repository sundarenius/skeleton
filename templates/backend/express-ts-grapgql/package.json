{
  "name": "@connect-platform/server",
  "version": "1.0.0",
  "description": "New backend node ts project",
  "author": "",
  "license": "ISC",
  "main": ".dist/src/bin/www.js",
  "private": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "run:scrape": "ts-node ./src/crawler/scrape-anything.ts",
    "dev:debug": "APP_NAME=instagram_creator NODE_ENV=development ts-node ./src/bin/www.ts",
    "dev": "APP_NAME=instagram_creator NODE_ENV=development tsc-watch --onSuccess \"node dist/src/bin/www.js\"",
    "build": "tsc",
    "start:dev": "yarn build && SERVE_STATIC=true APP_NAME=instagram_creator NODE_ENV=development node dist/src/bin/www.js",
    "start:test": "yarn build && SERVE_STATIC=true APP_NAME=instagram_creator NODE_ENV=test node dist/src/bin/www.js",
    "start:staging": "yarn build && SERVE_STATIC=true APP_NAME=instagram_creator NODE_ENV=staging node dist/src/bin/www.js",
    "start:prod": "yarn build && SERVE_STATIC=true APP_NAME=instagram_creator NODE_ENV=production node dist/src/bin/www.js",
    "lint:check": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "refresh-local-dbs": "sh -c './drop-all-local-dbs.sh' && node ./init-db.js"
  },
  "keywords": [],
  "dependencies": {
    "axios": "^1.6.0",
    "cloudflare-scraper": "^2.0.0",
    "crypto": "^1.0.1",
    "puppeteer": "^21.4.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "bcrypt": "^5.1.1",
    "cli-color": "^2.0.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^11.11.0",
    "graphql": "^16.8.1",
    "http-errors": "^1.8.0",
    "jade": "^1.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongodb": "^4.1.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "2",
    "socket.io": "^4.7.2",
    "source-map-support": "^0.5.19",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cli-color": "^2.0.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.9",
    "@types/node": "^16.0.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.3.5"
  }
}
